            android:label="RegisterActivity" >
	</activity>
	<activity
            android:name="com.example.myfirstapp.GCMMainActivity"
            android:configChanges="orientation|keyboardHidden"
            android:label="GCMMainActivity" >
        </activity>

        <receiver
            android:name="com.example.myfirstapp.GcmBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.example.myfirstapp.c2dm.intent.RECEIVE" />
                <action android:name="com.example.myfirstapp.c2dm.intent.REGISTRATION" />

                <category android:name="com.example.myfirstapp" />
            </intent-filter>
        </receiver>

        <service android:name="com.example.myfirstapp.GCMNotificationIntentService" />
        
