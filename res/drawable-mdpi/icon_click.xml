<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  
  <item android:state_pressed="true">
    <shape>
      <solid
          android:color="#0000"/>
          
      <stroke
          android:width="2dp"
          android:color="#e9fc32" />
      <margin
          android:left="2dp"
          android:top="2dp"
          android:right="2dp"
          android:bottom="2dp"/>
    </shape>
  </item>
</selector>